<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="Kobberlan:InspirationUrl" value="https://lan.logicwork.net" />
    <add key="Kobberlan:GamesDirectory" value="Games" />
    <add key="Torrent:CreateTorrentFiles" value="false" />
    <add key="Port:TorrentTrackerHTTP" value="23457" />
    <add key="Port:TorrentTrackerUDP" value="23458" />
    <add key="Port:TorrentClient" value="23459" />
    <add key="Port:BroadcastUDP" value="23456" />
    <add key="Port:CommunicationTCP" value="23456" />  
    <add key="Desharp:Enabled" value="1" />
    <add key="Desharp:Output" value="html" />
    <add key="Desharp:Levels" value="exception,debug, -info,-notice,warning,error,critical,alert,emergency" />
    <add key="Desharp:Directory" value="~/Logs" />
    <add key="CommunicationSocket:WatchDogTimeOut" value="30000" />
    <add key="CommunicationSocket:SendTimeout" value="15000" />
    <add key="CommunicationSocket:ReceiveTimeout" value="15000" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Mono.Nat" publicKeyToken="6c9468a3c21bc6d1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
